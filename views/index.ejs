<html>
<head>
  <title>Node Dash</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <style type="text/css">
  #video-container{
    position: inherit;
  }
  video{
    height: 300px;
  }
  video::-webkit-media-controls-enclosure {
    display: none !important;
  }

  .media-information{
    display: none;
    position: absolute;
    top: 0px;
    left: 0;
    z-index: 2147483647;
    color: white;
    width: 170px;
    padding-top: 4px;
    padding-left: 5px;
    opacity: .6;
    background-color: black;
  }
  .loading-bar{
    position: absolute;
    display: none;
    left: 0px;
    top: 0px;
    height: 4px;
    background-color: black;
    z-index: 2147483647;
  }
  .current-loading{
    background-color: #2780e3;
    width: 0%;
    height: 4px;
  }
  </style>
</head>
<body>
  <div class="col-md-12">
    <h2>Node Dash</h2>
    <h5 id="title">Cargando video ... </h5>
    <div id="video-container"></div>
  </div>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script src="/js/dash-client.js"></script>
  <script>

    var videoData = {
      url: "https://www.youtube.com/watch?v=JAUoeqvedMo",
      sequence : "w2.14;136;136;137;137;137;137;137;137;137;137;137;137;137",
      audio_sequence : "140;140;140;140;140;140;140;140;140;140;140;140;140",
      video_length: 60
    }

    $(document).ready(function(){
      var video = document.getElementById('video-container');
      var dashMerken = new DashClient(videoData, video ,function(){ 
        console.log('video loaded.');
        $("#title").text('Video cargado: ' + videoData.url);
      });
    });
  </script>
</body>
</html>